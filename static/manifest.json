{
  "manifest_version": 2,
  "name": "Invisible History",
  "description": "Make your certain broswer history invisible",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "bookmark-16.png"
  },
  "icons": {
    "16": "bookmark-16.png",
    "48": "bookmark-48.png",
    "128": "bookmark-128.png"
  },
  "permissions": [
    "storage",
    "history",
    "tabs"
  ]
}
